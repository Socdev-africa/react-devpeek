{
  "name": "react-devpeek",
  "version": "0.1.0",
  "description": "A developer tool for debugging React state and local storage",
  "type": "module",
  "main": "dist/react-devpeek.umd.js",
  "module": "dist/react-devpeek.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run test && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "demo": "vite serve demo --config demo-vite.config.ts",
    "build:demo": "vite build --config demo-vite.config.ts",
    "deploy:demo": "npm run build:demo && gh-pages -d demo/dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Socdev-africa/react-devpeek.git"
  },
  "keywords": [
    "react",
    "developer-tools",
    "local-storage",
    "state-management",
    "debugging",
    "zustand",
    "context-api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Socdev-africa/react-devpeek/issues"
  },
  "homepage": "https://github.com/Socdev-africa/react-devpeek#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.0.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "storybook": "^7.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2",
    "vitest": "^0.34.0"
  },
  "dependencies": {
    "framer-motion": "^10.15.0",
    "lodash": "^4.17.21",
    "zustand": "^4.3.9"
  },
  "overrides": {
    "@types/react": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  }
}